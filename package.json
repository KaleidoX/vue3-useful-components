    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "element-plus": "^2.4.2",
    "lodash-es": "^4.17.21",
    "mpegts.js": "^1.7.3",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "universal-cookie": "^6.1.1",
    "vant": "^4.8.0",
    "video.js": "^8.6.1",
    "vue": "^3.3.9",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.147",
    "@iconify/utils": "^2.1.12",
    "@rushstack/eslint-patch": "^1.6.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.10.0",
